{
  "name": "parliament",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/arkime/arkime.git"
  },
  "scripts": {
    "bundle": "(cd vueapp && vite build --minify false)",
    "bundle:min": "(cd vueapp && vite build --minify esbuild)",
    "addtestuser": "cd ../ && npm run viewer:addtestuser",
    "run_bundle": "cd ../ && npm run parliament:run_bundle",
    "dev": "cd ../ && npm run parliament:dev",
    "dev_front": "cd vueapp && vite",
    "dev_back": "cd ../ && npm run parliament:dev_back",
    "lint": "cd ../ && npm run parliament:lint"
  },
  "engines": {
    "node": ">= 20.9.0 < 23",
    "npm": ">= 3.0.0"
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules",
      "vueapp",
      "*.json"
    ],
    "watch": [
      ".",
      "../common",
      "../tests/parliament.ini"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "@vue/compat": "^3.5.18",
    "@vue/server-renderer": "^3.5.18",
    "vite": "^6.3.5",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/compiler-sfc": "^3.5.18",
    "bootstrap": "^5.3.7",
    "bootstrap-vue-next": "^0.30.4",
    "unplugin-vue-components": "^28.8.0",
    "vue-loader": "^17.4.2"
  }
}
