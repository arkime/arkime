{
  "license": "Apache-2.0",
  "scripts": {
    "bundle": "(cd vueapp && vite build --minify false)",
    "bundle:min": "(cd vueapp && vite build --minify esbuild)",
    "addtestuser": "cd ../ && npm run viewer:addtestuser",
    "run_bundle": "cd ../ && npm run cont3xt:run_bundle",
    "predev": "cd ../ && npm run viewer:addtestuser",
    "dev": "concurrently 'npm run dev_front' 'npm run dev_back'",
    "dev_front": "cd vueapp && vite",
    "dev_back": "NODE_ENV=development nodemon cont3xt.js -c ../tests/cont3xt.ini",
    "doc": "cd ../ && npm run cont3xt:doc",
    "lint": "cd ../ && npm run cont3xt:lint",
    "lint-fix": "cd ../ && npm run cont3xt:lint-fix"
  },
  "engines": {
    "node": ">= 20.9.0 < 23",
    "npm": ">= 3.0.0"
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules",
      "vueapp"
    ],
    "watch": [
      ".",
      "../common",
      "../tests/cont3xt.ini"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "@ckpack/vue-color": "^1.6.0",
    "@mdi/font": "^7.4.47",
    "email-deep-validator": "^3.3.0",
    "extract-domain": "^4.1.5",
    "maxmind": "^4.3.22",
    "punycode": "^2.3.1",
    "vue-debounce": "^5.0.0",
    "vue-draggable-next": "^2.2.1",
    "vuetify": "^3.9.7",
    "vuex-persistedstate": "^4.1.0",
    "whois-json": "^2.0.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/vue": "^8.1.0",
    "@vue/compiler-sfc": "^3.4.27",
    "jsdom": "^24.1.0",
    "vite-plugin-vuetify": "^2.1.2",
    "vue-axios": "^3.5.2",
    "vue-json-pretty": "^2.4.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-darwin-arm64": "^4.24.0",
    "@rollup/rollup-linux-x64-gnu": "^4.24.0"
  }
}
