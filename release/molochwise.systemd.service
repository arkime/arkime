[Unit]
Description=Moloch WISE
After=network.target

[Service]
Type=simple
Restart=on-failure
StandardOutput=tty
ExecStart=/bin/sh -c 'MOLOCH_INSTALL_DIR/bin/node wiseService.js -c MOLOCH_INSTALL_DIR/etc/wise.ini >> MOLOCH_INSTALL_DIR/logs/wise.log 2>&1'
WorkingDirectory=MOLOCH_INSTALL_DIR/wiseService

[Install]
WantedBy=multi-user.target
