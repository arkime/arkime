  "scripts": {
    "addtestuser": "node addUser.js -c ../tests/config.test.ini -n testuser admin admin admin --admin --packetSearch",
    "build": "npm run bundle:min",
    "start": "node viewer.js -c ../config.ini",
    "start:dev": "node viewer.js -c config.dev.ini & webpack -d --watch --progress --hide-modules --display-error-details --config vueapp/build/webpack.dev.conf.js",
    "start:test": "node viewer.js --debug -c ../tests/config.test.ini -n test & webpack -d --watch --progress --hide-modules --display-error-details --config vueapp/build/webpack.dev.conf.js",
    "start:testuser": "nodemon viewer.js -c ../tests/config.test.ini -n testuser & webpack -d --watch --progress --hide-modules --display-error-details --config vueapp/build/webpack.dev.conf.js",
    "start:testuserdebug": "node viewer.js -c ../tests/config.test.ini -n testuser --debug & webpack -d --watch --progress --hide-modules --display-error-details --config vueapp/build/webpack.dev.conf.js",
    "start:multiviewer": "node multies.js -c ../tests/config.test.ini -n all",
    "start:testmultiviewer": "node viewer.js -c ../tests/config.test.ini -n testmulties & webpack -d --watch --progress --hide-modules --display-error-details --config vueapp/build/webpack.dev.conf.js",
    "bundle": "webpack --progress --hide-modules -d --display-error-details --bail --config vueapp/build/webpack.dev.conf.js",
    "bundle:min": "node vueapp/build/build.js",
    "prestart": "npm run bundle:min",
    "lint": "cd .. ; npm run lint-viewer"
  },
