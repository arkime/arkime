  },
  "dependencies": {
    "fs-ext": "^2.0.0"
  },
  "devDependencies": {
    "d3": "^7.7.0",
    "jquery": "^3.6.0",
    "save-svg-as-png": "^1.4.17"
  },
  "scripts": {
    "bundle": "cd ../ && npm run viewer:bundle",
    "bundle:min": "cd ../ && npm run viewer:build",
