    "viewer:dev": "concurrently 'npm run viewer:dev_back' 'npm run viewer:dev_front'",
    "viewer:dev_front": "(cd viewer && npm run dev_front)",
    "viewer:dev_back": "(cd viewer && NODE_ENV=development nodemon viewer.js -c ../tests/config.test.ini -n testuser)",
    "viewer:run_bundle": "(cd viewer && nodemon viewer.js -c ../tests/config.test.ini -n testuser)",
